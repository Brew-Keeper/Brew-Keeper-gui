{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "strict": 0,
        "quotes": 0,
        "no-unused-vars": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0,
        "comma-dangle": ["error", "only-multiline"],
        "eqeqeq": "error",
        "eol-last": [0],
        "no-mixed-requires": [0],
        "prefer-template": ["warn"]
    }
}
